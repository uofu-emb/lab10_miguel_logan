# expects the pico sdk to be in the home directory

~/.pico-sdk/openocd/0.12.0+dev/openocd -s ~/.pico-sdk/openocd/0.12.0+dev/scripts -f ~/.pico-sdk/openocd/0.12.0+dev/scripts/interface/cmsis-dap.cfg -f ~/.pico-sdk/openocd/0.12.0+dev/scripts/target/rp2040.cfg -c "adapter speed 1000" -c "init; reset halt"